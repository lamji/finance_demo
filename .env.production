# API Configuration
EXPO_PUBLIC_API_URL=https://debt-management-theta.vercel.app
EXPO_PUBLIC_API_TIMEOUT=30000

# Authentication
EXPO_PUBLIC_AUTH_PERSISTENCE_KEY=@auth_token

# Environment
EXPO_PUBLIC_NODE_ENV=production
EXPO_PUBLIC_APP_ENV=production

# App Configuration
EXPO_PUBLIC_APP_NAME=Debt Management
